{
    "name": "gravitymedia/uri",
    "description": "A PHP library for generating RFC 3986 compliant uniform resource identifiers (URI).",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Uri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\UriTest\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.idea",
            "/.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    }
}
